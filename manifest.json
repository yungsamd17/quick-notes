{
    "manifest_version": 3,
    "name": "Quick Notes",
    "description": "A simple note-taking extension",
    "version": "1.3",
    "author": "Sam @yungsamd17",
    "homepage_url": "https://github.com/yungsamd17/quick-notes",
    "icons":
    {
        "16": "src/assets/icon-16.png",
        "48": "src/assets/icon-48.png",
        "128": "src/assets/icon-128.png"
    },
    "permissions": ["storage"],
    "action":
    {
        "default_title": "Quick Notes",
        "default_popup": "popup.html"
    }
}